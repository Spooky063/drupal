{% if primary %}
	<h2 class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
	<nav{{attributes.addClass('menu-tasks','primary')}}>
		<ul>{{ primary }}</ul>
	</nav>
{% endif %}

{% if secondary %}
	<h2 class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
	<nav{{attributes.addClass('menu-tasks','secondary')}}>
		<ul>{{ secondary }}</ul>
	</nav>
{% endif %}
